{
  "compilerOptions": {
    "baseUrl": "/",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@footballerCard/*": [
        "@/app/components/FootballerCardPage/*"
      ],
      "@footballField/*": [
        "@/app/components/FootballFieldPage/*"
      ],
      "@marketPlace/*": [
        "@/app/components/MarketPlacePage/*"
      ],
      "@navbar/*": [
        "@/app/components/Navbar/*"
      ],
      "@paginator/*": [
        "@/app/components/Paginator/*"
      ],
      "@playerCard/*": [
        "@/app/components/PlayerCard/*"
      ],
      "@img/*": [
        "@/app/static/img/*"
      ],
      "@fonts/*": [
        "@/app/static/fonts/*"
      ],
      "@store/*": [
        "@/app/store/*"
      ],
      "@types/*": [
        "@/app/types/*"
      ],
      "@routes/*": [
        "src/app/routes/*"
      ],
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "ScriptHost"
    ],
    "importHelpers": true,
    "sourceMap": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "jsx": "react-jsx",
    // "outDir": "./dist/"
  },
  // "include": [
  //     "src/**/**/**/*.ts",
  //     "src/**/**/*.ts",
  // ]
}