{
  "compilerOptions": {
    "baseUrl": "/",
    "paths": {
      "@FootballerCard": [
        "./src/app/components/FootballerCardPage/*"
      ],
      "@FootballField": [
        "./src/app/components/FootballFieldPage/*"
      ],
      "@MarketPlace": [
        "./src/app/components/MarketPlacePage/*"
      ],
      "@Navbar": [
        "./src/app/components/Navbar/*"
      ],
      "@Paginator": [
        "./src/app/components/Paginator/*"
      ],
      "@PlayerCard": [
        "./src/app/components/PlayerCard/*"
      ],
      "@Img": [
        "./src/app/static/img/*"
      ],
      "@Store": [
        "./src/app/store/*"
      ],
      "@Types": [
        "./src/app/types/*"
      ],
      "@Routes": [
        "./src/app/routes/*"
      ],
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "ScriptHost"
    ],
    "importHelpers": true,
    "sourceMap": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "jsx": "react-jsx",
    // "outDir": "./dist/"
  },
  // "include": [
  //     "src/**/**/**/*.ts",
  //     "src/**/**/*.ts",
  // ]
}